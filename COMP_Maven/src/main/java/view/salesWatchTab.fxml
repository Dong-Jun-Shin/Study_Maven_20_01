						<ImageView fx:id="productImageView33" fitHeight="170.0" fitWidth="210.0" onMouseClicked="#imageSel" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="3" GridPane.rowIndex="3" />
					</children>
				</GridPane>
				<HBox alignment="CENTER" prefHeight="110.0" prefWidth="200.0" spacing="50.0">
					<children>
						<Button fx:id="btnLast" mnemonicParsing="false" onAction="#btnPageMove" prefHeight="23.0" prefWidth="100.0" text="이전">
							<font>
								<Font size="15.0" />
							</font>
						</Button>
						<Button fx:id="btnNext" mnemonicParsing="false" onAction="#btnPageMove" prefWidth="100.0" text="다음">
							<font>
								<Font size="15.0" />
							</font>
						</Button>
					</children>
				</HBox>
			</children>
		</VBox>
	</children>
</HBox>
