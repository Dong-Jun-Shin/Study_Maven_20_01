<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="750.0" prefWidth="1100.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SalesWatchTabController">
	<padding>
		<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
	</padding>
	<children>
		<TreeView fx:id="productTreeView" prefHeight="730.0" prefWidth="250.0" />
		<VBox prefHeight="730.0" prefWidth="850.0">
			<children>
				<GridPane alignment="CENTER" gridLinesVisible="true" prefHeight="730.0" prefWidth="820.0">
					<columnConstraints>
						<ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
						<ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
						<ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
						<ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
					</columnConstraints>
					<rowConstraints>
						<RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES" />
					</rowConstraints>
					<children>
						<ImageView fx:id="productImageView00" fitHeight="170.0" fitWidth="210.0" onMouseClicked="#imageSel" pickOnBounds="true" preserveRatio="true" />
						<ImageView fx:id="productImageView10" fitHeight="170.0" fitWidth="210.0" onMouseClicked="#imageSel" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="1" />
						<ImageView fx:id="productImageView20" fitHeight="170.0" fitWidth="210.0" onMouseClicked="#imageSel" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="2" />
						<ImageView fx:id="productImageView30" fitHeight="170.0" fitWidth="210.0" onMouseClicked="#imageSel" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="3" />
						<ImageView fx:id="productImageView01" fitHeight="170.0" fitWidth="210.0" onMouseClicked="#imageSel" pickOnBounds="true" preserveRatio="true" GridPane.rowIndex="1" />
						<ImageView fx:id="productImageView11" fitHeight="170.0" fitWidth="210.0" onMouseClicked="#imageSel" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="1" GridPane.rowIndex="1" />
						<ImageView fx:id="productImageView21" fitHeight="170.0" fitWidth="210.0" onMouseClicked="#imageSel" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="2" GridPane.rowIndex="1" />
						<ImageView fx:id="productImageView31" fitHeight="170.0" fitWidth="210.0" onMouseClicked="#imageSel" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="3" GridPane.rowIndex="1" />
						<ImageView fx:id="productImageView02" fitHeight="170.0" fitWidth="210.0" onMouseClicked="#imageSel" pickOnBounds="true" preserveRatio="true" GridPane.rowIndex="2" />
						<ImageView fx:id="productImageView12" fitHeight="170.0" fitWidth="210.0" onMouseClicked="#imageSel" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="1" GridPane.rowIndex="2" />
						<ImageView fx:id="productImageView22" fitHeight="170.0" fitWidth="210.0" onMouseClicked="#imageSel" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="2" GridPane.rowIndex="2" />
						<ImageView fx:id="productImageView32" fitHeight="170.0" fitWidth="210.0" onMouseClicked="#imageSel" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="3" GridPane.rowIndex="2" />
						<ImageView fx:id="productImageView03" fitHeight="170.0" fitWidth="210.0" onMouseClicked="#imageSel" pickOnBounds="true" preserveRatio="true" GridPane.rowIndex="3" />
